{
  "name": "almond-launcher",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "9",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "postcss": "7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "wait-on": "^6.0.0",
